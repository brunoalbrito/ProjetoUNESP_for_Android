<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.mackezie.projetounesp_for_android" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <!--NÂO ESQUEÇA DE ELIMINAR A LINHA android:label="@string/app_name"-->

        <!--AQUI TEMOS COMO MUDAR O NOME DA ACTIVITY-->
        <activity
            android:name=".MainActivity"
            android:label="@string/ActivityInicio"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".FAEActivity"
            android:label="@string/ActivityFAE"
            >
        </activity>
    </application>

</manifest>
